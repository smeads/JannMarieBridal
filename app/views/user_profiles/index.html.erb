<br>
<div class="jumbotron">
  <div class="col-sm-8">
    <h1>Client Profiles</h1>
    <p>Measure twice cut once</p>
  </div>
  <div class="text-center">
    <h2><%= image_tag("logos/JMBL.png", width: '200px') %></h2>
  </div>
</div>
<% @user_profiles.each do |user_profile| %>
  <div class="media">
    <div class="media-body">
      <h3 class="media-heading">
        <div class="text-center">
        <%= link_to user_profile.full_name, user_profile %>
        </div>
      </h3>
    </div>
  </div>
<% end %>
</div>
